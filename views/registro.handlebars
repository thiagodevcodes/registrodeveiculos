<div class="registros-container">

<table class="">
    <tr>
        <th>Cliente</th>
        <th>Marca</th>
        <th>Modelo</th>
        <th>Placa</th>
        <th>Data</th>
        <th>Hora Entrada</th>
        <th>Data Saida</th>
        <th>Hora Saida</th>
    </tr>
{{#each posts}}
    <tr>
        <td>{{nome}}</td>
        <td>{{marca}}</td>
        <td>{{modelo}}</td>
        <td>{{placa}}</td>
        <td>{{dataEntrada}}</td>
        <td>{{horaEntrada}}</td>
        <td>{{dataSaida}}</td>
        <td>{{horaSaida}}</td>
    </tr>

{{/each}}
</table>
